<div class="container">
  <div class="row">
    <div class="span12">
      <h1>Listing Selections</h1>
    </div>
  </div>
  
  <div class="row">
  <%
    @selections.each do |selection|
      if selection.name != "Owned Nail Polishes"
  %>
      <div class="span6">
        <%= render 'selection', :selection => selection %>
      </div>
  <%
      end
    end
  %>
  </div>
</div>
